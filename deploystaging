node{
   stage('SCM Checkout'){
     git 'https://github.com/afraj1/website'
   }
   
   stage('Deploy to Tomcat'){
      
      sshagent (credentials:['stagingwithroot']) {
         sh 'scp -o StrictHostKeyChecking=no /var/lib/jenkins/workspace/Afraj-website/index.html root@10.0.2.160:/home/ec2-user'
      }
   }
   
 }