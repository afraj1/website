node{
   stage('SCM Checkout'){
     git 'https://github.com/afraj1/website'
   }
   
   stage('Deploy to Tomcat'){
      
      sshagent(['staging-root']) {
         sh 'scp -o StrictHostKeyChecking=no /var/lib/jenkins/workspace/Afraj-website/index.html ec2-user@10.0.2.160:/home/ec2-user'
      }
   }
   
 }