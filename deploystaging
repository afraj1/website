node{
   stage('SCM Checkout'){
     git 'https://github.com/afraj1/website'
   }
   
   stage('Deploy to Tomcat'){
      
      sshagent(['tomcat-dev']) {
         sh 'scp -o StrictHostKeyChecking=no /var/lib/jenkins/workspace/Afraj-website/index.html root@3.222.189.33:/home/ec2-user'
      }
   }
   
 }